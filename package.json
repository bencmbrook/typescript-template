{
  "name": "@bencmbrook/typescript-template",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "1.0.0",
  "description": "TODO",
  "license": "UNLICENSED",
  "bin": "./dist/bin/cli.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "del dist && tsc --project tsconfig.build.json",
    "script": "touch .env.local && tsx --env-file=.env.local",
    "cli": "tsx bin/cli.ts",
    "test": "tsx --test test/**.test.ts",
    "lint": "pnpm run --parallel --aggregate-output \"/^lint:*/\"",
    "lint:prettier": "prettier --check . --log-level warn",
    "lint:types": "tsc --noEmit",
    "lint:test-types": "tsc --noEmit --project test/tsconfig.json",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build && pnpm publint"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "22.x",
    "del-cli": "^5.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "prettier": "^3.4.2",
    "publint": "^0.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "dependencies": {
    "consola": "^3.2.3",
    "zod": "^3.23.8"
  }
}
